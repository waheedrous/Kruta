@model HPE.Kruta.Model.Queue

   

<div class="col-sm-4">
    <b>Document Details</b><br /><br />

    Recorded Date: @String.Format("{0:MM-dd-yyyy}", Model.Document.RecordedDateTime )<br />
    Document Number: @Html.DisplayFor(m => m.Document.DocumentNumber)<br />
    Document Type: @Html.DisplayFor(m => m.Document.DocumentSubType.DocumentType.Description)<br /><br />


</div>
<div class="col-sm-4">
    <b>Property Detials</b><br /><br />

    Parcel Number: @Html.DisplayFor(m => m.Property.ParcelNumber)<br />
    Legal Description: @Html.DisplayFor(m => m.Property.FullLegalDescription)<br />
    <br />
    Class Code: @Html.DisplayFor(m => m.Property.PropertyClass.Description)<br /> <br />
</div>

<div class="col-sm-4">
    @foreach (var note in Model.QueueNotes)
    {
        <table>
            <tr>
                <td>@string.Format("{0} {1:MM-dd-yyyy hh:mm}: {2}", "WA", note.CreatedOn, note.Note)</td>
               
               

            </tr>
        </table>
    }
</div>